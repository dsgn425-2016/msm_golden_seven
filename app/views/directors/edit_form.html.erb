<h1>Modify a Director</h1>
<form class="" action="/edit_director/<%=@director.id%>">
  <div>
  <input type="text" name="name" value="<%= @director.name%>">
</div>
<div class="">
  <input type="text" name="dob" value="<%= @director.dob%>">
</div>
<div class="">
  <input type="text" name="image_url" value="<%= @director.image_url%>">
</div>
<div class="">
  <input type="long text" name="bio" value="<%= @director.bio%>">
</div>
<input type="submit" value="Modify Now">
</form>
