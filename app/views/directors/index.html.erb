<h1>Directors</h1>
<hr>

<a href="/directors/new_form">ADD A NEW DIRECTOR </a>

<table>
  <thead>
    <tr>
      <th>
        Image
      </th>
      <th>
        Dob
      </th>
      <th>
        Name
      </th>
      <th>
        Bio
      </th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @list_of_directors.each do |director| %>
    <tr>
      <td>
        <img src="<%= director.image_url %>" width="50">
      </td>
      <td>
        <%= director.dob %>
      </td>
      <td>
        <%= director.name %>
      </td>
      <td>
        <%= director.bio %>
      </td>
      <td>
        <a href="/directors/<%= director.id %>">Show</a>
      </td>
      <td>
        <a href="/directors/<%= director.id %>/edit_form">Edit</a>
      </td>
      <td>
        <a href="/delete_director/<%= director.id %>">delete</a>
      </td>
    </tr>
    <% end %>
  </tbody>
</table>
