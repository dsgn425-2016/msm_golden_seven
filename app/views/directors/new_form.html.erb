<div class="page-header">
  <h1>Add Director</h1>
</div>

<form action="/create_director" class="">

  <div class="">
    <label for="name" class="">
      Name
    </label>
    <div class="">
      <input type="text" id="name" name="name" placeholder="Name" class="">
    </div>
  </div>

  <div class="">
    <label for="dob" class="">
      Dob
    </label>
    <div class="">
      <input type="text" id="dob" name="dob" placeholder="Dob" class="">
    </div>
  </div>

  <div class="">
    <label for="bio" class="">
      Bio
    </label>
    <div class="">
      <textarea id="bio" name="bio" placeholder="Bio" class="" rows="3"></textarea>
    </div>
  </div>

  <div class="">
    <label for="image_url" class="">
      Image url
    </label>
    <div class="">
      <input type="text" id="image_url" name="image_url" placeholder="Image url" class="form-control">
    </div>
  </div>

  <div class="">
    <div class="">
      <input type="submit" value="Create Director" class="">
    </div>
  </div>
</form>
