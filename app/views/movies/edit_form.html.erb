<h1>Modify a Movie</h1>
<form class="" action="/edit_movie/<%=@movie.id%>">
  <div>
  <input type="text" name="name" value="<%= @movie.title%>">
</div>
<div class="">
  <input type="text" name="dob" value="<%= @movie.year%>">
</div>
<div class="">
  <input type="text" name="image_url" value="<%= @movie.image_url%>">
</div>
<div class="">
  <input type="long text" name="bio" value="<%= @movie.duration%>">
</div>
<div class="">
  <input type="long text" name="bio" value="<%= @movie.description%>">
</div>
<input type="submit" value="Modify Now">
</form>
