<h1>Add Actor</h1>
<form action="/create_actor">
  <label for="dob">Dob</label>
  <input type = "text" id="dob" placeholder="Dob" name="the_dob">

  <div><label for="name">Name</label>
    <input type = "text" id="name" placeholder="Name" name="the_name"></div>

    <div><label for="bio">Bio</label>
      <input type = "text" id="bio" placeholder="Bio" name="the_bio"></div>

      <div><label for="image_url">Image url</label>
        <input type = "text" id="image_url" placeholder="Image url" name="the_image"></div>

        <button>Create Actor</button>
      </form>
