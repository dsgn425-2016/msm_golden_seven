<h1>Actors</h1>

    <a href="/actors/new_form">
    Add New Actor </a>
<table>

  <tr>
    <th>Image</th>
    <th>Dob</th>
    <th>Name</th>
    <th>Bio</th>
  </tr>
  <% @actors.each do |actor| %>
  <tr>
    <td><img src="<%= actor.image_url %>" width="80"></td>
    <td><%= actor.dob %></td>
    <td><%= actor.name %></td>
    <td><%= actor.bio %></td>
    <td>      <a href="/actors/<%= actor.id %>">
      View</a>
    </td>
    <td>      <a href="/delete_actor/<%= actor.id %>">
      Delete</a>
    </td>
    <td>      <a href="/actors/<%= actor.id %>/edit_form">
      Modify</a>
    </td>

  </tr>
  <% end %>

</table>
